#include"stdio.h"
int main()
{
	int a[100],b[10000]={0};
int i,n,k,c=0,g=0;
scanf("%d",&n);
for(i=0;i<n;i++)
scanf("%d",&a[i]);

for(i=0;i<n;i++)

for(k=i+1;k<n;k++)
{


b[c]=a[i]+a[k];

c++;
}
for(i=0;i<n;i++)


{for(c=0;c<10000;c++)

{if(a[i]==b[c])
{
g+=1;
break;
}}


}
printf("%d",g);
return 0;
}
